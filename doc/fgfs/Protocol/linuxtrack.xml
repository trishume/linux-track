<?xml version="1.0"?>

<PropertyList>

	<generic>

		<input>

			<line_separator>newline</line_separator>
			<var_separator>tab</var_separator>

			<chunk>
				<name>heading</name>
				<type>float</type>
				<node>/sim/current-view/goal-heading-offset-deg</node>
			</chunk>

			<chunk>
				<name>pitch</name>
				<type>float</type>
				<node>/sim/current-view/goal-pitch-offset-deg</node>
			</chunk>

			<chunk>
				<name>roll</name>
				<type>float</type>
				<node>/sim/current-view/goal-roll-offset-deg</node>
			</chunk>

			<chunk>
				<name>x</name>
				<type>float</type>
				<node>/sim/current-view/target-x-offset-m</node>
			</chunk>

			<chunk>
				<name>y</name>
				<type>float</type>
				<node>/sim/current-view/target-y-offset-m</node>
			</chunk>

			<chunk>
				<name>z</name>
				<type>float</type>
				<node>/sim/current-view/field-of-view</node>
				<offset>55.0</offset>
			</chunk>

		</input>

	</generic>

</PropertyList>
