<?xml version="1.0"?>

<PropertyList>

	<generic>

		<input>

			<line_separator>newline</line_separator>
			<var_separator>tab</var_separator>

			<chunk>
				<name>heading</name>
				<type>float</type>
				<node>/sim/linuxtrack/data/h</node>
			</chunk>

			<chunk>
				<name>pitch</name>
				<type>float</type>
				<node>/sim/linuxtrack/data/p</node>
			</chunk>

			<chunk>
				<name>roll</name>
				<type>float</type>
				<node>/sim/linuxtrack/data/r</node>
			</chunk>

			<chunk>
				<name>x</name>
				<type>float</type>
				<node>/sim/linuxtrack/data/x</node>
			</chunk>

			<chunk>
				<name>y</name>
				<type>float</type>
				<node>/sim/linuxtrack/data/y</node>
			</chunk>

			<chunk>
				<name>z</name>
				<type>float</type>
				<node>/sim/linuxtrack/data/z</node>
			</chunk>

		</input>

	</generic>

</PropertyList>
